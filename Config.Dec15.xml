            fast_rise="5"
            fast_gap="5"
            fast_threshold="220"
            energy_rise="25"
            energy_gap="25"
            slow_rise="20"
            slow_gap="20"
            slow_threshold="20"/> 
        <Processor name="McpProcessor" />
    </DetectorDriver>

